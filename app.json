{
  "name": "Telegraph Bot",
  "description": "Telegram bot to upload media to Telegra.ph",
  "logo": "https://te.legra.ph/file/21cb991708199f9b061ea.jpg",
  "keywords": [
    "telegram",
    "bot",
    "python",
    "pyrogram"
  ],
    "buildpacks": [{
    "url": "heroku/python"
  }],
  "formation": {
    "worker": {
      "quantity": 1,
      "size": "free"
    }
  },
  "repository": "https://github.com/DARKEMPIRESL/Telegra.ph-Uploader",
  "addons": [
    {
       "options": {
          "version": "12"
       },
       "plan": "heroku-postgresql"
    }
  ],

  "env": {
    "ENVIRONMENT": {
      "description": "Don't change ANYTHING.",
      "required": true,
      "value": "ANYTHING"
    },
    "API_ID": {
      "description": "Get this value from my.telegram.org.",
      "required": true,
      "value": "16884543"
    },
    "API_HASH": {
      "description": "Get this value from my.telegram.org.",
      "required": true,
      "value": "cc4535cefe1fa2e167d75a5e2ac24246"
    },
    "BOT_TOKEN": {
      "description": "Obtain a Telegram bot token by contacting @BotFather",
      "required": true,
      "value": "5399109106:AAGl5w1hc8GF-_-Pkr_6mDcWqhVPU3F4eog"
    },
    "MUST_JOIN": {
      "description": "Not compulsory. Obtain your Telegram Chat's username/id",
      "required": false,
      "value": "@NightVission"
    },
    "OWNER_ID": {
      "description": "Obtain your Telegram ID",
      "required": true,
      "value": "5196689118"
    }

  }
}
